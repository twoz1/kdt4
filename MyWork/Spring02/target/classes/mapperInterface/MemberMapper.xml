<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapperInterface.MemberMapper" >
	<!-- ** Jo_List 추가 -->
	<select id="joList" resultType="domain.MemberDTO">
		select * from member where jno=#{jno}
	</select>
	<!-- ** selectList -->
	<select id="selectList" resultType="domain.MemberDTO">
	  select * from member
	</select>
	<!-- ** selectOne -->
	<select id="selectOne" resultType="domain.MemberDTO">
		select * from member where id=#{id}
	</select>
	<!-- ** insert -->
	<insert id="insert">
		insert into member values(#{id},#{password},#{name},#{age},#{jno},#{info},
		#{point},#{birthday},#{rid})
	</insert>
	<!-- ** update -->
	<update id="update">
		update member set name=#{name}, age=#{age}, jno=#{jno}, info=#{info},
		 point=#{point}, birthday=#{birthday}, rid=#{rid} where id=#{id}
	</update>
	<!-- ** delete -->
	<delete id="delete">
		delete from member where id=#{id}
	</delete>
</mapper>